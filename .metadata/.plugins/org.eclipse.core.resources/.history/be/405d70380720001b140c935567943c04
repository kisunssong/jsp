<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<html>
<head>
<script type="text/javascript">
var kor,eng,met,sum,avg
kor=prompt("국어점수: ")
eng=prompt("영어점수: ")
met=prompt("수학점수: ")
kor=parseInt(kor)
eng=parseInt(eng)
met=parseInt(met)
sum=kor+eng+met
avg = sum/3.0
document.write(avg)
if(avg >= 90){
document.write("A")
}
else if(avg >= 80) {
document.write("B")
}
		
</script>
</head>
<body>

